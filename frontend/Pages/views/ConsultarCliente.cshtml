@page
@model TallerAutos.App.Frontend.Pages.ConsultarCliente
@{
    ViewData["Title"] = "Consultar Cliente";
}



@Html.Partial("_navView");
<div class="conteinerDiv">

    <div class="divConteiner">
        <div class="divImg">
            <img src="~/images/carBackground.jpg" alt="">
        </div>
        <div class="divForm">
            <form>
                <label for="IdSearch">Buscar: </label>
                <input type="text" id="IdSearch">
                <input type="submit" value="Buscar">
            </form>
        </div>
    </div>
    <table>
        <thead>
            <tr>
                <th>numero documento</th>
                <th>nombres</th>
                <th>apellidos</th>
                <th>telefono</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var cliente in Model.clientes)
            {
                <tr>
                    <td>@cliente.numeroDocumento</td>
                    <td>@cliente.nombre</td>
                    <td>@cliente.apellido</td>
                    <td>@cliente.telefono</td>
                </tr>
            }
        </tbody>
    </table>
</div>
